<div class="container">
  <h1>Restaurants</h1>
  <%= link_to "Add restaurant" , new_restaurant_path %>
    <ul class="list-group">
      <% @restaurants.each do |restaurant| %>
        <li class="list-group-item"><%= link_to restaurant.name, restaurant_path(restaurant)  %>
      <% end %>
    </ul>
</div>
